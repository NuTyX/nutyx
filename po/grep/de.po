msgid "Usage: %s [OPTION]... PATTERNS [FILE]...\n"
msgstr "Aufruf: %s [OPTION]… MUSTER [DATEI]…\n"

#: src/grep.c:1973
#, c-format
msgid "Try '%s --help' for more information.\n"
msgstr "„%s --help“ liefert weitere Informationen.\n"

#: src/grep.c:1979
#, c-format
